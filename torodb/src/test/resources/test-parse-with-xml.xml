<config>
  <generic>
    <logLevel>NONE</logLevel>
    <logPackages>
      <com.torodb>DEBUG</com.torodb>
    </logPackages>
  </generic>
  <protocol>
    <mongo>
      <net>
        <port>27019</port>
      </net>
      <replication>
        <replication>
          <replSetName>rs1</replSetName>
          <role>HIDDEN_SLAVE</role>
          <syncSource>localhost:27017</syncSource>
        </replication>
      </replication>
    </mongo>
  </protocol>
  <backend>
    <postgres>
      <host>localhost</host>
      <port>5432</port>
      <user>root</user>
    </postgres>
  </backend>
</config>
