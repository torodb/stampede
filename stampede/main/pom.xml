                    <plugin>
                        <groupId>io.fabric8</groupId>
                        <artifactId>docker-maven-plugin</artifactId>
                        <extensions>true</extensions>
                        <configuration>
                            <images>
                                <image>
                                    <alias>torodb-stampede</alias>
                                    <name>torodb/stampede:%v</name>
                                    <build>
                                        <from>torodb/base:latest</from>
                                        <tags>
                                            <tag>${project.version}</tag>
                                            <tag>latest</tag>
                                        </tags>
                                        <assembly>
                                            <mode>dir</mode>
                                            <descriptor>assembly.xml</descriptor>
                                        </assembly>
                                        <env>
                                            <PATH>/maven/bin:$PATH</PATH>
                                        </env>
                                        <runCmds>
                                            <run>useradd -d /maven torodb</run>
                                            <run>chown torodb /maven -R</run>
                                        </runCmds>
                                        <entryPoint>
                                            <exec>
                                                <arg>/maven/bin/entry-point</arg>
                                            </exec>
                                        </entryPoint>
                                        <cmd>
                                            <arg>torodb-stampede</arg>
                                        </cmd>
                                    </build>
                                </image>
                            </images>
                        </configuration>
                        <executions>
                            <execution>
                                <id>docker-build</id>
                                <phase>package</phase>
                                <goals>
                                    <goal>build</goal>
                                </goals>
